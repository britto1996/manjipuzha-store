{
  "name": "online-store",
  "version": "1.0.0",
  "description": "store",
  "main": "index.js",
  "type": "module",
  "engine": {
    "node": "14.x",
    "npm": "6.x"
  },
  "scripts": {
    "start": "node btm-store-backend/index.js",
    "server": "nodemon btm-store-backend/index.js",
    "client": "npm run --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node btm-store-backend/seeder",
    "data:destroy": "node btm-store-backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/britto1996/manjipuzha-store.git"
  },
  "author": "btm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/britto1996/manjipuzha-store/issues"
  },
  "homepage": "https://github.com/britto1996/manjipuzha-store#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.7",
    "serve": "^11.3.2"
  }
}
